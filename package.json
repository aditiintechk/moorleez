{
	"name": "moorleez",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint",
		"postinstall": "prisma generate",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:e2e": "playwright test",
		"test:e2e:local": "dotenv -e .env.test -- playwright test",
		"test:e2e:ui": "dotenv -e .env.test -- playwright test --ui",
		"test:e2e:headed": "dotenv -e .env.test -- playwright test --headed",
		"seed:test": "dotenv-cli -e .env.test -- npx prisma db seed",
		"validate": "npm run type-check && npm run lint && npm run test:unit && npm run build",
		"type-check": "tsc --noEmit",
		"test:unit": "vitest run",
		"pre-push": "npm run validate",
		"prepare": "husky"
	},
	"dependencies": {
		"@clerk/nextjs": "^6.36.0",
		"@neondatabase/serverless": "^1.0.2",
		"@prisma/adapter-neon": "^7.0.0",
		"@prisma/client": "^7.0.0",
		"cloudinary": "^2.8.0",
		"dotenv": "^17.2.3",
		"next": "^16.0.7",
		"prisma": "^7.0.0",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-hook-form": "^7.66.1",
		"recharts": "^3.6.0",
		"resend": "^6.6.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.57.0",
		"@tailwindcss/postcss": "^4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^5.1.1",
		"@vitest/ui": "^4.0.15",
		"dotenv-cli": "^11.0.0",
		"eslint": "^9",
		"eslint-config-next": "16.0.3",
		"husky": "^9.1.7",
		"jsdom": "^27.2.0",
		"tailwindcss": "^4",
		"tsx": "^4.20.6",
		"typescript": "^5",
		"vitest": "^4.0.15"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	}
}
